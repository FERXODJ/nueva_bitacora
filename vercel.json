{
    "version": 2,
    "builds": [
      {
        "src": "bitacora/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "bitacora/wsgi.py"
      }
    ],
    "build": {
      "command": "./.vercel_build.sh && pip install -r requirements.txt"
    }
  }